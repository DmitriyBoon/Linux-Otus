{{ hostname }} {{ name_router1 }}
!
{{ interface }} {{ router1_eth0 }}
 ipv6 nd suppress-ra
!
interface {{ router1_eth1 }}
 ip address {{ local_ip_1 }}
 ipv6 nd suppress-ra
!
interface lo
!
interface {{ router1_lo }}
 ipv6 nd suppress-ra
!
interface {{ router1_vlan12_if }} 
 ip address {{ router1_vlan12_ip }}
 ipv6 nd suppress-ra
!
interface {{ router1_vlan16_if }} 
 ip address {{ router1_vlan16_net }}
 ipv6 nd suppress-ra
!
!
!
line vty
!